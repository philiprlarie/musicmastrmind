<b class="interpretation-creator"><%= interpretation.creator().escape('username') %></b>
<abbr class="timeago interpretation-created_at"
  title="<%= interpretation.escape('created_at') %>"
  ><%= interpretation.escape('created_at') %></abbr>

<br>
<%= interpretation.escape('body') %>

<% if (interpretation.belongsToCurrentUser && !turnOffButtons) { %>
  <br/>
  <button type="button" class="interpretation-delete-btn btn btn-default">Delete</button>
  <button type="button" class="interpretation-edit-btn btn btn-default">Edit</button>
<% } %>

<div class="interpretation-edit"></div>
