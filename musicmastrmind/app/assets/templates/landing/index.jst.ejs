<% var count = 0; %>
<% songs.each( function (song) { %>
  <% if (count < 6) { %>
  <li class="landing-page-item">
     <div class="landing-index-links">
        <a href="#/songs/<%= song.id %>" class="landing-index-song-title">
           <%= song.escape('title') %>
        </a> -
        <a class="landing-index-artist-link"
           href="#/artists/<%= song.artist().id %>">
          <%= song.artist().escape('name') %>
        </a>
     </div>

         <a href="#/songs/<%= song.id %>" class="landing-index-image">
           <% var image_url = song.artist().escape("image_url") || "http://res.cloudinary.com/dnbxm7cx2/image/upload/v1440450454/r8rctpkcwhzwc5swewnv.jpg"; %>
           <% image_url = image_url.replace("upload/", "upload/w_400,h_250,c_fill/"); %>
           <img src="<%= image_url %>" alt="" />
         </a>

  </li>
  <% } %>
  <% count += 1; %>
<% }); %>
